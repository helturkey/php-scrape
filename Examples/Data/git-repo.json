{
  "version": 4,
  "extraction": {
    "resultPipeline": [
      {
        "configuration": {
          "property": "repo_name",
          "xpaths": [
            ".//h3[not(@id)][not(@style)][contains(concat(' ',normalize-space(@class),' '),\" repo-list-name \")]/a[1][not(@id)][not(@class)][not(@style)]"
          ]
        }
      },
      {
        "configuration": {
          "property": "repo_url",
          "type": "URL",
          "xpaths": [
            ".//h3[not(@id)][not(@style)][contains(concat(' ',normalize-space(@class),' '),\" repo-list-name \")]/a[1][not(@id)][not(@class)][not(@style)]"
          ]
        }
      },
      {
        "configuration": {
          "property": "description",
          "xpaths": [
            ".//p[not(@id)][not(@style)][contains(concat(' ',normalize-space(@class),' '),\" repo-list-description \")]"
          ]
        }
      },
      {
        "configuration": {
          "property": "stars_today",
          "xpaths": [
            ".//p[not(@id)][not(@style)][contains(concat(' ',normalize-space(@class),' '),\" repo-list-meta \")]"
          ],
          "regexp": "/(\\d*)\\s[stars]/"
        }
      }
    ],
    "resultXPaths": [
      "//div[@class=\"explore-content\"]"
    ],
    "rowXPaths":[
      "//*[contains(@class,\"repo-list-item\")]"
    ]
  },
  "urlProperties": [
    "repo_url"
  ],
  "playback": {
    "url": "${webpage/url}",
    "fixHtml": true
  }
}