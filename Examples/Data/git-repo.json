{
  "version": 4,
  "extraction": {
    "resultPipeline": [
      {
        "configuration": {
          "property": "repo_name",
          "xpaths": [
            ".//div[1]/h3/a"
          ]
        }
      },
      {
        "configuration": {
          "property": "repo_url",
          "type": "URL",
          "xpaths": [
            ".//div[1]/h3/a"
          ]
        }
      },
      {
        "configuration": {
          "property": "description",
          "xpaths": [
            ".//div[@class=\"py-1\"]/p"
          ]
        }
      },
      {
        "configuration": {
          "property": "stars_today",
          "xpaths": [
            ".//div[4]/span[@class=\"float-right\"]"
          ],
          "regexp": "/(\\d*)\\s[stars]/"
        }
      }
    ],
    "resultXPaths": [
      "//div[@class=\"explore-content\"]"
    ],
    "rowXPaths":[
      "//*[contains(@class,\"repo-list\")]/li"
    ]
  },
  "urlProperties": [
    "repo_url"
  ],
  "playback": {
    "url": "${webpage/url}",
    "fixHtml": true
  }
}